@model UserModel

@{
    Layout = null;
}
@if (Model == null)
{
    <li>
        Что- пошло не так!
    </li>
}
else
{
    <div>
        <a class="nav-link text-dark" asp-action="GetUserData" asp-controller="Account">Пользователь: @Model.Email</a>
    </div>
    <div>
        <a class="nav-link text-dark" asp-controller="Account" asp-action="Logout">Logout</a>
    </div>
}
